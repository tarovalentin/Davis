@article{KLEISS1989616,
title = "Multigluon cross sections and 5-jet production at hadron colliders",
journal = "Nuclear Physics B",
volume = "312",
number = "3",
pages = "616 - 644",
year = "1989",
issn = "0550-3213",
doi = "https://doi.org/10.1016/0550-3213(89)90574-9",
url = "http://www.sciencedirect.com/science/article/pii/0550321389905749",
author = "Ronald Kleiss and Hans Kuijf",
abstract = "We present a calculation of the cross section for the multigluon scattering process gg → ggggg. The helicity subamplitudes are computed using recursion in the number of gluons. A new algebraic identity is presented which allows for a complete reduction of the subamplitudes to a minimal basis, leading to a considerable increase in the efficiency of the algorithm. The results are compared with various approximate expressions which have recently been proposed, under several sets of realistic experimental conditions. The computational complexity of the algorithm is discussed in detail."
}

@article{BERENDS1988759,
title = "Recursive calculations for processes with n gluons",
journal = "Nuclear Physics B",
volume = "306",
number = "4",
pages = "759 - 808",
year = "1988",
issn = "0550-3213",
doi = "https://doi.org/10.1016/0550-3213(88)90442-7",
url = "http://www.sciencedirect.com/science/article/pii/0550321388904427",
author = "F.A. Berends and W.T. Giele",
abstract = "A method is presented in which multigluon processes are calculated recursively. The technique is explicitly developed for processes where only gluons are produced and processes where in addition to the gluons also a quark-antiquark pair with or without a vector boson or e+e− pair are present. The recursion relations are used to derive rigorously amplitudes for certain configurations, where most of the gluons have the same helicities. This proves a number of conjectures in the literature. Also expressions for amplitudes with collinear or soft gluons are derived."
}

